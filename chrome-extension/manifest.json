{
   "name": "Bookmark This Page",
   "manifest_version": 2,
   "description": "Add this website to your bookmarks",
   "version": "0.1",
   "homepage_url": "https://rivario.com/bookmark",

   "permissions": [ "tabs", "http://*/*", "https://*/*" ],

   "background": {
      "scripts" : ["background.js"]
   },

   "browser_action": {
      "default_icon": "icon-19.png"
   },
   "icons": {
      "128": "icon-128.png",
      "16": "icon-16.png",
      "48": "icon-48.png"
   }
}
